<div class="table">
	<table class="listing" cellpadding="0" cellspacing="0">
		<tr>
							<th>created_at</th>
							<th>updated_at</th>
							<th>featured</th>
							<th>sketch_id</th>
							<th>image_file_name</th>
							<th>image_content_type</th>
							<th>image_file_size</th>
							<th>image_updated_at</th>
					</tr>
		<% @sketch_images.each do |sketch_image| -%>
			<tr>
									<td>
													<%= sketch_image.created_at %>
											</td>
									<td>
													<%= sketch_image.updated_at %>
											</td>
									<td>
													<%= sketch_image.featured %>
											</td>
									<td>
													<%= sketch_image.sketch_id %>
											</td>
									<td>
													<%= sketch_image.image_file_name %>
											</td>
									<td>
													<%= sketch_image.image_content_type %>
											</td>
									<td>
													<%= sketch_image.image_file_size %>
											</td>
									<td>
													<%= sketch_image.image_updated_at %>
											</td>
							</tr>
    <% end %>
		<% if @sketch_images.empty? %>
      <tr>
				<td colspan="8">No records</td>
      </tr>
    <% end %>
  </table>
</div>

<div class="actions">
	<a href="<%= new_admin_sketch_image_path %>" class="action add">New SketchImage</a>
</div>
